let SessionLoad = 1
let s:so_save = &g:so | let s:siso_save = &g:siso | setg so=0 siso=0 | setl so=-1 siso=-1
let v:this_session=expand("<sfile>:p")
silent only
silent tabonly
cd ~/.config/awesome
if expand('%') == '' && !&modified && line('$') <= 1 && getline(1) == ''
  let s:wipebuf = bufnr('%')
endif
let s:shortmess_save = &shortmess
if &shortmess =~ 'A'
  set shortmess=aoOA
else
  set shortmess=aoO
endif
badd +12 ~/.config/awesome/rc.lua
badd +49 ~/.config/awesome/themes/summer/theme.lua
badd +138 ~/.config/awesome/themes/summer/init.lua
badd +10 ~/.config/awesome/themes/summer/util.lua
badd +1 ~/.config/awesome/themes/summer/widgets/cpu.lua
badd +29 themes/summer/widgets/power.lua
argglobal
%argdel
edit ~/.config/awesome/themes/summer/init.lua
let s:save_splitbelow = &splitbelow
let s:save_splitright = &splitright
set splitbelow splitright
wincmd _ | wincmd |
vsplit
1wincmd h
wincmd w
wincmd _ | wincmd |
split
1wincmd k
wincmd w
let &splitbelow = s:save_splitbelow
let &splitright = s:save_splitright
wincmd t
let s:save_winminheight = &winminheight
let s:save_winminwidth = &winminwidth
set winminheight=0
set winheight=1
set winminwidth=0
set winwidth=1
exe 'vert 1resize ' . ((&columns * 102 + 102) / 204)
exe '2resize ' . ((&lines * 44 + 45) / 90)
exe 'vert 2resize ' . ((&columns * 101 + 102) / 204)
exe '3resize ' . ((&lines * 43 + 45) / 90)
exe 'vert 3resize ' . ((&columns * 101 + 102) / 204)
argglobal
balt themes/summer/widgets/power.lua
let s:l = 208 - ((41 * winheight(0) + 44) / 88)
if s:l < 1 | let s:l = 1 | endif
keepjumps exe s:l
normal! zt
keepjumps 208
normal! 034|
wincmd w
argglobal
if bufexists(fnamemodify("~/.config/awesome/themes/summer/theme.lua", ":p")) | buffer ~/.config/awesome/themes/summer/theme.lua | else | edit ~/.config/awesome/themes/summer/theme.lua | endif
if &buftype ==# 'terminal'
  silent file ~/.config/awesome/themes/summer/theme.lua
endif
balt ~/.config/awesome/themes/summer/widgets/cpu.lua
let s:l = 49 - ((35 * winheight(0) + 22) / 44)
if s:l < 1 | let s:l = 1 | endif
keepjumps exe s:l
normal! zt
keepjumps 49
normal! 029|
wincmd w
argglobal
if bufexists(fnamemodify("~/.config/awesome/themes/summer/widgets/cpu.lua", ":p")) | buffer ~/.config/awesome/themes/summer/widgets/cpu.lua | else | edit ~/.config/awesome/themes/summer/widgets/cpu.lua | endif
if &buftype ==# 'terminal'
  silent file ~/.config/awesome/themes/summer/widgets/cpu.lua
endif
balt ~/.config/awesome/themes/summer/theme.lua
let s:l = 5 - ((4 * winheight(0) + 21) / 43)
if s:l < 1 | let s:l = 1 | endif
keepjumps exe s:l
normal! zt
keepjumps 5
normal! 042|
wincmd w
exe 'vert 1resize ' . ((&columns * 102 + 102) / 204)
exe '2resize ' . ((&lines * 44 + 45) / 90)
exe 'vert 2resize ' . ((&columns * 101 + 102) / 204)
exe '3resize ' . ((&lines * 43 + 45) / 90)
exe 'vert 3resize ' . ((&columns * 101 + 102) / 204)
tabnext 1
if exists('s:wipebuf') && len(win_findbuf(s:wipebuf)) == 0 && getbufvar(s:wipebuf, '&buftype') isnot# 'terminal'
  silent exe 'bwipe ' . s:wipebuf
endif
unlet! s:wipebuf
set winheight=1 winwidth=20
let &shortmess = s:shortmess_save
let &winminheight = s:save_winminheight
let &winminwidth = s:save_winminwidth
let s:sx = expand("<sfile>:p:r")."x.vim"
if filereadable(s:sx)
  exe "source " . fnameescape(s:sx)
endif
let &g:so = s:so_save | let &g:siso = s:siso_save
set hlsearch
nohlsearch
doautoall SessionLoadPost
unlet SessionLoad
